{
    "env": {
        "es6": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "new-cap": ["error", { "capIsNew": false }],
        "indent": ["error", 4],
        "no-unused-vars": ["error", { "varsIgnorePattern": "^m$" }],
        "require-jsdoc": 0
    }
}