{
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "new-cap": ["error", { "capIsNew": false }],
        "indent": ["error", 4],
        "require-jsdoc": 0
    }
}